{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "User": {
    "dataSource": "mongodb"
  },
  "AccessToken": {
    "dataSource": "mongodb",
    "public": false
  },
  "ACL": {
    "dataSource": "mongodb",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "mongodb",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "mongodb",
    "public": false
  },
  "Post": {
    "dataSource": "mongodb",
    "public": true
  },
  "Person": {
    "dataSource": "mongodb",
    "public": true
  },
  "Comment": {
    "dataSource": "mongodb",
    "public": true
  },
  "container": {
    "dataSource": "loopback-component-storage",
    "public": true
  },
  "Image": {
    "dataSource": "mongodb",
    "public": true
  },
  "ImageShortlink": {
    "dataSource": "redis",
    "public": true
  },
  "Email": {
    "dataSource": "emailer"
  },
  "endpoint": {
    "dataSource": "db",
    "public": true
  }
}
